            "validation": {},
            "serial": 8,
            "label": "Specifications"
          },
          {
            "identifier": "status",
            "input_type": "string",
            "field_type": "list",
            "validation": {
              "fixed_list_elements": ["Active", "Inactive", "Out of Stock"]
            },
            "serial": 9,
            "label": "Status"
          },
          {
            "identifier": "store_location",
            "input_type": "geo",
